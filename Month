print("Enter the month : ")
m=int(input())

print("Enter the year : ")
y=int(input())
if(m==1 or m==3 or m==5 or m==7 or m==8 or ==10 or m==12):
      print("\nMonth has 31 days. ")
elif(m==2 and (y%4==0):
      print("\nMonth has  days. ")
elif(m==2):
      print("\nMonth has 28 days. ")
else: 
      print("\nMonth has 30 days. ")
      
